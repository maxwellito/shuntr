{
  "name": "Shuntr",
  "version": "0.1.0",
  "manifest_version": 2,
  "description": "Boilerplate to define redirection rules",
  "homepage_url": "http://github.com/maxwellito/shuntr",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "http://example.com/*"
  ],
  "background": {
    "scripts": ["js/background.js"],
    "persistent": true
  }
}
